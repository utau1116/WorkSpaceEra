@MESSAGE_PALAMCNG_B2
IF NOWEX:6
	IF !GETBIT(CFLAG:異常経験,2)
		EXP:異常経験 ++
		SETBIT CFLAG:異常経験,2
	ENDIF
	PRINTL 四 重 絶 頂
	PRINTL (それぞれ8倍の珠が得られます)
ELSEIF NOWEX:5
	SELECTCASE NOWEX:5
		CASE 1
			PRINTL Ｃ＆Ｖ＆Ａ絶頂
			PRINTL (それぞれ4倍の珠が得られます)
		CASE 2
			PRINTL Ｂ＆Ｖ＆Ａ絶頂
			PRINTL (それぞれ4倍の珠が得られます)
		CASE 3
			PRINTL Ｃ＆Ｂ＆Ａ絶頂
			PRINTL (それぞれ4倍の珠が得られます)
		CASE 4
			PRINTL Ｃ＆Ｖ＆Ｂ絶頂
			PRINTL (それぞれ4倍の珠が得られます)
	ENDSELECT
ELSEIF NOWEX:4
	SELECTCASE NOWEX:4
		CASE 1
			PRINTL Ｃ＆Ｖ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
		CASE 2
			PRINTL Ｃ＆Ａ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
		CASE 3
			PRINTL Ｖ＆Ａ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
		CASE 4
			PRINTL Ｃ＆Ｂ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
		CASE 5
			PRINTL Ｖ＆Ｂ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
		CASE 6
			PRINTL Ａ＆Ｂ絶頂
			PRINTL (それぞれ2倍の珠が得られます)
	ENDSELECT
ELSEIF NOWEX:3
	IF NOWEX:3 == 2
		PRINTL 強絶頂Ｂ
	ELSE
		PRINTL 絶頂Ｂ
	ENDIF
ELSEIF NOWEX:2
	IF NOWEX:2 == 2
		PRINTL 強絶頂Ａ
	ELSE
		PRINTL 絶頂Ａ
	ENDIF
ELSEIF NOWEX:1
	IF NOWEX:1 == 2
		PRINTL 強絶頂Ｖ
	ELSE
		PRINTL 絶頂Ｖ
	ENDIF
ELSEIF NOWEX:0
	IF NOWEX:0 == 2
		PRINTL 強絶頂Ｃ
	ELSE
		PRINTL 絶頂Ｃ
	ENDIF
ENDIF

;絶頂による欲望ＬＶアップ
LOCAL = 0
SIF NOWEX:0 >= 2 || NOWEX:3 >= 2 || NOWEX:1 || NOWEX:2
	LOCAL = 1
SIF NOWEX:1 >= 2 || NOWEX:2 >= 2
	LOCAL = 2
SIF (NOWEX:0 >= 2 || NOWEX:3 >= 2) && NOWEX:4
	LOCAL = 2
SIF NOWEX:5
	LOCAL = 3
SIF NOWEX:6
	LOCAL = 4
;自制心があると１下がる
SIF TALENT:自制心
	LOCAL -= 1

IF ABL:欲望 < LOCAL
	ABL:欲望 ++
	PRINTFORML そして欲望がLV{LOCAL}になった
	;欲望の上昇による[抑圧]の消滅をチェック
	CALL YOKUBO_UP_CHECK
ENDIF
PRINTL 
;FileName_TRACHECK_ORGASM.ERB ---------------------- Rev1.00
;調教対象の噴乳チェック処理
;CALL		USER
;ARG		ARG:キャラNO
